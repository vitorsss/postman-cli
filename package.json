{
  "name": "@vitorsss/postman-cli",
  "version": "1.0.7",
  "description": "Postman CLI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "npm run test:unit",
    "test:unit": " NODE_ENV=test jest --config jest.config.js --coverage",
    "lint": "npm run lint:js",
    "lint:js": "eslint 'src/**/*.ts'",
    "lint:fix": "npm run lint -- --fix && npm run prettier:fix",
    "prettier:fix": "prettier --write --config ./.prettierrc .",
    "cmd": "ts-node ./bin.ts",
    "dist": "npm run build && tsc-alias --project tsconfig.json",
    "gen": "ts-node ./gen.ts",
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "rm -rf dist"
  },
  "homepage": "https://github.com/vitorsss/postman-cli",
  "author": "Vitor Soares Silva <silva.vitor.soares@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitorsss/postman-cli.git"
  },
  "keywords": [
    "postman",
    "cli",
    "api"
  ],
  "license": "Apache-2.0",
  "bin": {
    "pm": "./dist/bin.js",
    "postman": "./dist/bin.js"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "axios-rate-limit": "^1.3.0",
    "axios-retry": "^3.2.5",
    "commander": "^9.2.0",
    "config-yaml": "^1.1.6",
    "deepmerge": "^4.2.2",
    "enquirer": "^2.3.6",
    "js-yaml": "^4.1.0",
    "postman-collection": "^4.1.2"
  },
  "devDependencies": {
    "@types/config-yaml": "^1.1.1",
    "@types/jest": "27.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/json-schema": "^7.0.11",
    "@types/nested-error-stacks": "2.1.0",
    "@types/node": "17.0.31",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "eslint": "8.15.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "json-schema": "^0.4.0",
    "json-schema-to-typescript": "^10.1.5",
    "prettier": "2.6.2",
    "ts-jest": "28.0.2",
    "ts-node": "10.7.0",
    "tsc-alias": "^1.6.7",
    "tsconfig-paths": "^4.0.0",
    "typescript": "4.6.4"
  }
}
